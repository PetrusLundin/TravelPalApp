<Window x:Class="TravelPalApp.SignUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravelPalApp"
        mc:Ignorable="d"
        Title="SignUpWindow" Height="450" Width="350">
    <Grid>
        <StackPanel>
            <Label
                Content="Username"
                HorizontalAlignment="Center"
                />
            <TextBox
                x:Name="txtUsername"
                Height="20"
                Width="100"/>
            <Label
                Content="Password"
                HorizontalAlignment="Center"
                />
            <TextBox
                x:Name="txtPassword"
                Height="20"
                Width="100"/>
            <Button
                x:Name="btnRegister"
                Content="Register"
                Width=" 80"
                Margin="0,20,0,0" Click="btnRegister_Click"/>
        </StackPanel>
    </Grid>
</Window>